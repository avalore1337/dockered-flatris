#!groovy

properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'master'
  }
  trigger {pollSCM('* * * * * *')}
  options {
    timestamps()
  }
  stages {
    stage('create docker image') {
      steps {  
        echo '================ start building image ================='
        sh 'docker build .'
      }
    }
  }
}
